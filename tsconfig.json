{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
		"allowJs": true,
		"checkJs": true,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"resolveJsonModule": true,
		"skipLibCheck": true,
		"sourceMap": true,
		"strict": true,
		"typeRoots": ["./node_modules/@types", "./types"],
		"moduleResolution": "node",
		"baseUrl": ".",
		"paths": {
			"$lib/*": ["src/lib/*"]
		}
	},
	"include": ["src/**/*"]
}

